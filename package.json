{
  "name": "node-ts-template",
  "version": "0.0.3",
  "description": "A Node.js template for using TypeScript/ts-node with NodeJS",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm tsc",
    "dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
    "start": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "node",
    "pnpm",
    "ts-node",
    "typescript"
  ],
  "author": "Will Walsh",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2"
  }
}
