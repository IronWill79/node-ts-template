{
  "name": "node-ts-template",
  "version": "0.0.4",
  "description": "A TypeScript Node.js template",
  "main": "dist/index.js",
  "scripts": {
    "build": "pnpm tsc",
    "dev": "nodemon --watch \"*.ts\" --exec \"ts-node\" ./src/index.ts",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "start": "ts-node ./src/index.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "eslint",
    "node",
    "pnpm",
    "ts-node",
    "typescript"
  ],
  "author": "Will Walsh",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
}
